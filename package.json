    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.1",
    "@types/react": "^18.2.37",
    "@types/node": "^20.9.0",
    "typescript": "^5.2.2"
  }
