{
  "name": "ollama-chat-hub",
  "version": "1.0.0",
  "description": "A powerful interface for chatting with local Ollama models and managing their tools, features, and artifacts",
  "main": "backend/main.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "nodemon backend/main.js",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "node backend/main.js"
  },
  "keywords": [
    "ollama",
    "chat",
    "ai",
    "mcp",
    "tools",
    "nextjs",
    "tailwindcss"
  ],
  "author": "Ollama Chat Hub",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "zustand": "^4.4.7",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-highlight": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "prismjs": "^1.29.0",
    "react-icons": "^4.12.0",
    "framer-motion": "^10.16.4",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "chokidar": "^3.5.3",
    "node-fetch": "^3.3.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "fs-extra": "^11.1.1",
